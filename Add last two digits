import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner s = new Scanner(System.in);
	    int number = s.nextInt();
	    int last = number % 10;
      int first = number;
      while (first >= 10) {
        first /= 10;
      }
      System.out.println(first + last);
	}
}
